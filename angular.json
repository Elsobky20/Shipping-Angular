{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Shipping-Angular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/shipping-angular",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "src/assets/vendors/mdi/css/materialdesignicons.min.css",
              "src/assets/vendors/ti-icons/css/themify-icons.css",
              "src/assets/vendors/css/vendor.bundle.base.css",
              "src/assets/vendors/font-awesome/css/font-awesome.min.css",
              "src/assets/css/style.css",
              "src/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.css"
            ],
            "scripts": [
              "src/assets/vendors/js/vendor.bundle.base.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/off-canvas.js",
              "src/assets/js/misc.js",
              "src/assets/js/settings.js",
              "src/assets/vendors/chart.js/chart.umd.js",
              "src/assets/vendors/bootstrap-datepicker/bootstrap-datepicker.min.js",
              "src/assets/js/todolist.js",
              "src/assets/js/jquery.cookie.js",
              "src/assets/js/dashboard.js"
            ],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Shipping-Angular:build:production"
            },
            "development": {
              "buildTarget": "Shipping-Angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
           "src/assets",
               {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
               },
               {
                "glob": "**/*",
                "input": "public"
              }
              ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
